<% @posts.each do |post| %>
  var posts = $('.feed');
  <% if post.photos.length == 1 %>
    posts.append("<%= j(render(partial: "photos/show", locals: { photo: post.photos.first })) %>");
  <% else %>
    posts.append("<%= j(render(partial: "posts/show", locals: { post: post, user: post.author })) %>");
  <% end %>
<% end %>
$('.feed').append('<%= paginate @posts %>');